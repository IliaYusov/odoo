<odoo>
    <data>


        <!-- project_office -->
        <record id="show_project_office_tree" model="ir.ui.view">
            <field name="name">project_budget.project_office.tree</field>
            <field name="model">project_budget.project_office</field>
            <field name="arch" type="xml">
                <tree string="project_office">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="descr"/>
                </tree>
            </field>
        </record>

        <record id="show_project_office_form" model="ir.ui.view">
            <field name="name">project_budget.project_office.form</field>
            <field name="model">project_budget.project_office</field>
            <field name="arch" type="xml">
                <form>
                    <sheet string="project_office">
                            <group>
                                    <field name="name"/>
                                    <field name="code"/>
                            </group>
                            <group>
                                <field name="descr" widget="html"/>
                            </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="show_project_office" model="ir.actions.act_window">
            <field name="name">project_office</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">project_budget.project_office</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'search_default_group_by_provider': True}</field>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Create a new project_office
              </p><p>
                each project budget have project_office
              </p>
            </field>
        </record>
<!-- end project_office-->
